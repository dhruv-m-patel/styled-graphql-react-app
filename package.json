{
  "name": "styled-react-app",
  "version": "1.0.0",
  "description": "A universal react app with styled-components support",
  "main": "build/server/index.js",
  "scripts": {
    "build:server": "babel ./src --out-dir build",
    "build:client": "BABEL_ENV=webpack webpack",
    "build": "npm run build:server && npm run build:client",
    "clean": "rimraf build && rimraf build-static",
    "start": "NODE_ENV=production node ./build/server/index.js",
    "start-dev": "babel-node src/server",
    "start:watch": "npm run clean && npm run build && nodemon",
    "lint:js": "eslint src/**/*.js && eslint src/**/*.jsx",
    "lint:css": "stylelint \"./src/**/*.jsx\"",
    "lint": "npm run lint:js && npm run lint:css",
    "pretest": "jest --clearCache",
    "test": "jest",
    "ci:test": "jest --updateSnapshot --coverage",
    "storybook": "start-storybook -p 3001",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dhruv-m-patel/universal-react-app.git"
  },
  "keywords": [
    "universal-react",
    "react"
  ],
  "author": "Dhruv Patel",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/dhruv-m-patel/universal-react-app/issues"
  },
  "homepage": "https://github.com/dhruv-m-patel/universal-react-app#readme",
  "dependencies": {
    "@babel/runtime": "^7.5.5",
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-brands-svg-icons": "^5.12.0",
    "@fortawesome/free-regular-svg-icons": "^5.12.0",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "@loadable/component": "^5.12.0",
    "@loadable/server": "^5.12.0",
    "bluebird": "^3.7.2",
    "body-parser": "^1.19.0",
    "confit": "^3.0.0",
    "cookie-parser": "^1.4.4",
    "core-js": "^3.6.4",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-enrouten": "^1.3.0",
    "fast-deep-equal": "^2.0.1",
    "fetch-everywhere": "^1.0.5",
    "meddleware": "^3.0.3",
    "morgan": "^1.9.1",
    "react": "^16.13.0",
    "react-bootstrap": "^1.0.0-beta.17",
    "react-dom": "^16.13.0",
    "react-helmet": "^5.2.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-api-middleware": "^3.2.0",
    "shortstop-handlers": "^1.0.1",
    "shortstop-regex": "0.0.1",
    "store": "^2.0.12",
